{
    "name": "the-don-himself/gremlin-ogm",
    "description": "A PHP Object Graph Mapper for Tinkerpop 3+ compatible Graph Databases (JanusGraph, Neo4j, etc.) that allows you to persist data and run gremlin queries.",
    "type": "library",
    "require": {
        "php": "^7.0",
        "brightzone/gremlin-php": "^3.0",
        "doctrine/common": "^2.8",
        "jms/serializer": "^2.0 || ^1.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Don Omondi",
            "email": "don.e.omondi@gmail.com"
        }
    ],
    "keywords": [
        "graph",
        "Amazon Neptune",
        "Azure Cosmos DB",
        "Datastax Enterprise Graph",
        "JanusGraph",
        "Neo4j",
        "Neo4j",
        "OrientDB",
        "gremlin-php",
        "gremlin-ogm"
    ],
    "autoload": {
        "psr-4": {
            "TheDonHimself\\GremlinOGM\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
