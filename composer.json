{
    "name": "the-don-himself/gremlin-ogm",
    "description": "A PHP Object Graph Mapper for Tinkerpop 3+ compatible Graph Databases (JanusGraph, Neo4j, etc.) that allows you to persist data and run gremlin queries.",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "abraham/twitteroauth": "^0.7.2",
        "brightzone/gremlin-php": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "guzzlehttp/psr7": "^1.4.2",
        "doctrine/common": "^2.8",
        "jms/serializer": "^1.9",
        "symfony/class-loader": "~4.0 || ~3.0 || ~2.7",
        "symfony/console": "~4.0 || ~3.0 || ~2.7",
        "symfony/yaml": "~4.0 || ~3.0 || ~2.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Don Omondi",
            "email": "don.e.omondi@gmail.com"
        }
    ],
    "keywords": [
        "graph",
        "Amazon Neptune",
        "Azure Cosmos DB",
        "Datastax Enterprise Graph",
        "JanusGraph",
        "Neo4j",
        "Neo4j",
        "OrientDB",
        "gremlin-php",
        "gremlin-ogm"
    ],
    "autoload": {
        "psr-4": {
            "TheDonHimself\\GremlinOGM\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "bin": ["bin/graph", "bin/graph.php"]
}
